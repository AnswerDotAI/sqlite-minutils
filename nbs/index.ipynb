    "user = users.upsert(user, pk='id')\n",
    "user"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "test(user['name'], 'Barn Owl', equals)\n",
    "test(user['age'], 102, equals)"
   ]
  },
  {
   "cell_type": "code",
